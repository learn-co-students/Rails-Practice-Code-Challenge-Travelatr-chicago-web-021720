<%= form_for @blogger do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<br>
<%= f.label :age %>
<%= f.text_field :age %>
<br>
<br>
<%= f.label :bio %>
<%= f.text_field :bio %>
<br>
<br>
<%= f.submit %>
<% end %>

<% @blogger.errors.to_a.each do |error| %>
  <p><%= error %></p>
<% end %>
